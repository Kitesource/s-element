{
  "name": "@smilekite/element",
  "version": "1.0.1",
  "description": "Vue3 components library",
  "author": {
    "name": "Smiley"
  },
  "license": "MIT",
  "type": "module",
  "keywords": [
    "Components",
    "Vue3",
    "Library"
  ],
  "homepage": "https://github.com/Kitesource/s-element",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kitesource/s-element"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/umd/s-element.umd.cjs",
  "module": "./dist/es/s-element.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/s-element.js",
      "require": "./dist/umd/s-element.umd.cjs"
    },
    "./dist": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p build-es build-umd",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "test": "vitest",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@algolia/client-search": "4.12",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5",
    "axios": "^1.7.2",
    "lodash-es": "^4.17.21",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitepress-demo-preview/component": "^2.3.0",
    "@vitepress-demo-preview/plugin": "^1.1.9",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^21.1.2",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.35",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.2.5",
    "search-insights": "2",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.5.4",
    "vite-plugin-eslint": "^1.8.1",
    "vitepress": "^1.0.0",
    "vitest": "^2.0.4",
    "vue-tsc": "^2.0.21"
  }
}